@{
    Layout = "~/Views/_ana.cshtml";
}
@using UniStaj
@using UniStaj.veri;
@model UniStaj.Models.BelgeGorModel
 


<style>
    .pdf {
        width: 100%;
        aspect-ratio: 4 / 3;
    } 
    .pdf,
    html,
    body {
        height: 100%;
        margin: 0;
        padding: 0;
    }

    h1,
    h3 {
        text-align: center;
    }

    h1 {
        color: green;
    }
</style>

<form id="formKayit" name="formKayit" autocomplete="off">
    @{
        int dilKimlik = Model.gecerliDil();
        bool eklemeIzniVarmi = (bool)ViewBag.eklemeIznivar;
        bool silmeIzniVarmi = (bool)ViewBag.silmeIznivar; 
        <body>
         
            <object class="pdf"
                    data="@Model.dosyasi.dosyaKonumu"
                    width="800"
                    height="500">
            </object>
        </body>

    }
</form> 


  
 
 